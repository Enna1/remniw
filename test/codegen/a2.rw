// RUN: %remniw %s -o %t ; lli %t | FileCheck %s
// RUN: %remniw %s -o %t -debug ; lli %t | FileCheck %s
// RUN: %remniw %s -o %t1 ; %remniw-llc %t1 -o %t2.s ; clang %t2.s -o %t3; %t3 | FileCheck %s

func bar(x *int) int {
    %dealloc(x);
    return 4;
}

func normalized() int {
    var foo func(*int) int;
    var x *int;
    var a1 **int;
    var m *int;
    var d int;
    var r int;

    %alloc(m, %sizeof int);
    foo = bar;
    x = &r;
    a1 = &x;
    d = *x;
    r = (foo)(m);
    return r;
}

func main() int {
    %output normalized();
    return 0;
}

// CHECK: 4
