// RUN: %remniw %s -o %t ; lli %t | FileCheck %s
// RUN: %remniw %s -o %t -debug ; lli %t | FileCheck %s
// RUN: %remniw %s -o %t1 ; %remniw-llc %t1 -o %t2.s ; clang %t2.s -o %t3; %t3 | FileCheck %s

func self(r int) int{
    return r;
}

func test(f func(int)int, a int) int {
    return (f)(a);
}

func test2(g func(int)int, b int) int {
    return (g)(b);
}

func main() int{
    var f,g func(int)int;
    f = self;
    g = self;
    %output test(f, 123) + test2(g, 321);
    return 0;
}

