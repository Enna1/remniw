// Emit LLVM IR
// RUN: %remniw -emit-llvm %s -o %t1 ; echo 10 | lli %t1 | FileCheck %s

// Emit X86 assembly
// RUN: %remniw-llc %t1 -o %t2.s ; clang %t2.s -o %t3; echo 10 | %t3 | FileCheck %s
// RUN: %remniw %s -o %t4 ; echo 10 | %t4 | FileCheck %s

// Emit RISCV assembly
// RUN: %remniw-llc --target=riscv %t1 -o %t2.riscv.s ; \
// RUN:     riscv64-unknown-linux-gnu-gcc -static %t2.riscv.s -o %t3.riscv.exe; \
// RUN:     echo 10 | qemu-riscv64 %t3.riscv.exe | FileCheck %s

func fib(n int) int {
    var f1,f2,i int;
    var temp int;
    f1 = 1;
    f2 = 1;

    i = n;
    while (i > 1) {
        temp = f1+f2;
        f1 = f2;
        f2 = temp;
        i = i-1;
    }
    return f2;
}

func main() int {
    var n int;
    n = %input;
    %output fib(n);
    return 0;
}

