// Emit LLVM IR
// RUN: %remniw -emit-llvm %s -o %t1 ; lli %t1 | FileCheck %s

// Emit X86 assembly
// RUN: %remniw-llc %t1 -o %t2.s ; clang %t2.s -o %t3; %t3 | FileCheck %s
// RUN: %remniw %s -o %t4 ; %t4 | FileCheck %s

// Emit RISCV assembly
// RUN: %remniw-llc --target=riscv %t1 -o %t2.riscv.s ; \
// RUN:     riscv64-unknown-linux-gnu-gcc -static %t2.riscv.s -o %t3.riscv.exe; \
// RUN:     qemu-riscv64 %t3.riscv.exe | FileCheck %s

func main() int {
  var x,y,z,a,b,c,t,u int;
  %output 123;
  x = f(87); // x = 555
  y = x; // y = 555
  z = x + 1; // z = 556
  t = f(x); // t = 555
  t = g(t); // t = 555
  u = g(123); // u = 123
  u = u; // u = 123
  %output z;
  if (z) {
    a = x; // a = 556
    b = f(y); // b = 555
    c = z; // c = 556
  } else {
    a = g(1);
    b = 2;
    c = 1;
  }
  %output x;
  %output y;
  %output z;
  %output a;
  %output b;
  %output c;
  %output t;
  %output u;
  return 0;
}

func f(x int) int { return 555; }

func g(p int) int { return p; }

func h() int { return 1234; }

// CHECK: 123
// CHECK-NEXT: 556
// CHECK-NEXT: 555
// CHECK-NEXT: 555
// CHECK-NEXT: 556
// CHECK-NEXT: 555
// CHECK-NEXT: 555
// CHECK-NEXT: 556
// CHECK-NEXT: 555
// CHECK-NEXT: 123
