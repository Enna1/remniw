// RUN: %remniw %s -o %t1  --enable-aphotic-shield ; %remniw-llc %t1 -o %t2.s
// RUN: %cxx_aphotic_shield %t2.s -o %t3
// RUN: %expect_crash %t3 2>&1 | FileCheck %s

// CHECK: APHOTIC-SHIELD detected a memory error
// CHECK: Double Free on address 0x{{[a-f0-9]+}}

func main() int {
    var y *int;
    %alloc(y, %sizeof int);
    %dealloc(y);
    %dealloc(y);
    return 0;
}
